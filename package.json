{
  "name": "reactish-state",
  "version": "2.0.1",
  "description": "Simple, decentralized (atomic) state management for React.",
  "author": "Zheng Song",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/szhsin/reactish-state.git"
  },
  "homepage": "https://github.com/szhsin/reactish-state#readme",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./types/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/",
    "types/",
    "middleware/",
    "plugin/",
    "shim/"
  ],
  "keywords": [
    "react",
    "state",
    "management",
    "redux"
  ],
  "scripts": {
    "start": "run-p watch \"tc -- --watch\"",
    "bundle": "rolldown -c",
    "watch": "rolldown -w -c",
    "clean": "rm -Rf dist types",
    "tc": "tsc",
    "prepare": "rm -Rf types/__tests__",
    "lint": "oxlint --type-aware .",
    "lint:es": "eslint .",
    "pret": "prettier -c .",
    "pret:fix": "prettier -w .",
    "build": "run-s pret clean tc lint lint:es bundle",
    "test": "vitest run",
    "test:watch": "vitest",
    "eg": "npm run dev --prefix examples"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/esm/index.mjs"
    },
    "./middleware": {
      "types": "./types/middleware/index.d.ts",
      "require": "./dist/cjs/middleware/index.cjs",
      "default": "./dist/esm/middleware/index.mjs"
    },
    "./middleware/immer": {
      "types": "./types/middleware/immer.d.ts",
      "require": "./dist/cjs/middleware/immer.cjs",
      "default": "./dist/esm/middleware/immer.mjs"
    },
    "./plugin": {
      "types": "./types/plugin/index.d.ts",
      "require": "./dist/cjs/plugin/index.cjs",
      "default": "./dist/esm/plugin/index.mjs"
    },
    "./shim": {
      "types": "./types/shim/index.d.ts",
      "require": "./dist/cjs/shim/index.cjs",
      "default": "./dist/esm/shim/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "peerDependencies": {
    "immer": ">=6.0",
    "react": "^16.8 || ^17 || ^18 || ^19"
  },
  "peerDependenciesMeta": {
    "immer": {
      "optional": true
    }
  },
  "dependencies": {
    "use-sync-external-store": "^1.6.0"
  },
  "devDependencies": {
    "@redux-devtools/extension": "^3.3.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/use-sync-external-store": "^1.5.0",
    "@vitest/coverage-istanbul": "^4.0.16",
    "@vitest/eslint-plugin": "^1.6.4",
    "deplift": "^1.0.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-hooks-addons": "^0.5.0",
    "globals": "^16.5.0",
    "immer": "^11.1.0",
    "jsdom": "^27.4.0",
    "npm-run-all": "^4.1.5",
    "oxlint": "^1.35.0",
    "oxlint-tsgolint": "^0.10.0",
    "prettier": "^3.7.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "rolldown": "^1.0.0-beta.57",
    "rollup-plugin-add-directive": "^1.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1",
    "vitest": "^4.0.16"
  }
}
